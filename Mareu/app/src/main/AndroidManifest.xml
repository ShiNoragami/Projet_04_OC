<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sn.maru">
    <application
        android:name=".MareuApplication"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.meeting_list.addmeet.AddMeetActivity"
            android:label="@string/addMeetingTitle"
            android:parentActivityName=".ui.meeting_list.mainactivity.MainActivity"
            android:theme="@style/ThemeWithActionBar"/>
        <activity android:name=".ui.meeting_list.mainactivity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>